const A = 100, B = 200, C = 300;
var x, y, z;
begin
    x := 1;
    y := 2;
    z := 3;
    if (A * 2) <> B then
    begin
        x := A;
        y := B;
        z := C;
        write x;
        write y;
        write z;
    end;
    x := 3000;
    y := 4000;
    z := 5000;
    write z;
    write y;
    write x;
end.
